{
  "name": "btb-server",
  "version": "0.0.1",
  "description": "btb server",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check",
    "lint:fix": "npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "pretest": "npm run rebuild",
    "posttest": "npm run lint",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "zicoz18 <zicoz18@ku.edu.tr>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^3.4.2",
    "@loopback/core": "^2.16.2",
    "@loopback/cron": "^0.5.2",
    "@loopback/repository": "^3.7.1",
    "@loopback/rest": "^10.0.0",
    "@loopback/rest-explorer": "^3.3.2",
    "@loopback/service-proxy": "^3.2.2",
    "@types/axios": "^0.14.0",
    "@types/crypto-js": "^4.0.2",
    "axios": "^0.21.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "loopback-connector-mongodb": "^5.5.0",
    "node-binance-api": "^0.12.5",
    "qs": "^6.10.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^10.17.60",
    "source-map-support": "^0.5.19",
    "typescript": "~4.3.4"
  }
}
